NGINX_SEPARATOR %{SPACE}-%{SPACE}
NGINX_REMOTE %{IPV4:remote_host}%{NGINX_SEPARATOR}%{DATA:remote_user}%{SPACE}
NGINX_LOG_DATE \[%{HTTPDATE:log_time}\]
NGINX_REQUEST %{SPACE}%{DATA:http_request}%{SPACE}%{NUMBER:http_status}%{SPACE}%{NUMBER:bytes_sent}%{SPACE}
NGINX_RESPONSE %{DATA:http_referer}%{SPACE}%{DATA:http_user_agent}
NGINX_LOG %{NGINX_REMOTE}%{NGINX_LOG_DATE}%{NGINX_REQUEST}%{NGINX_RESPONSE}

